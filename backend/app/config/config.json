{
    "specialization": {},
    "middleware": {
        "static": {
            "module": {
                "arguments": [
                    "path:./../public"
                ]
            }
        },
        "router": {
            "module": {
                "arguments": [
                    {
                        "directory": "path:./controllers"
                    }
                ]
            }
        },
        "authentication": {
            "enabled": false,
            "priority": 10,
            "route": "\/api\/v1((?!$))((?!\/root\/))((?!\/users))*",
            "module": {
                "name": "path:./middlewares/auth"
            }
        },
        "pagination": {
            "enabled": true,
            "priority": 20,
            "route": "/api",
            "module": {
                "name": "path:./middlewares/pagination"
            }
        }
    }
}
